{
    "name": "mandarinmedien/doctrine-date-types",
    "description": "A set of Doctrine 2 date types.",
    "keywords": ["doctrine", "orm", "database", "unix timestamp", "date"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Enrico Thies",
            "email": "et@mandarin-medien.de"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "doctrine/orm": "^2.6",
        "phpunit/phpunit": "^7.0 || ^8.0"
    },
    "autoload": {
        "psr-4": {"MandarinMedien\\DoctrineDateTypes\\" : "src/"}
    },
    "autoload-dev": {
        "psr-4": {"MandarinMedien\\DoctrineDateTypes\\Tests\\": "tests/"}
    },
    "config": {
        "sort-packages": true
    }
}
